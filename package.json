{
  "name": "react-full-stack",
  "version": "1.3.0",
  "description": " react-full-stack",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server  --mode development --devtool inline-source-map --hot",
    "server": "node ./src/server/server.js",
    "build": "webpack --mode production",
    "stopServer":"pm2 kill ",
    "startServer":"pm2 start ./src/server/server.js",
    "start": "npm run stopServer && npm run startServer  && npm run dev",
    "pro": "npm run stopServer && npm run startServer  && pm2 start ./bin/www.js"
  },
